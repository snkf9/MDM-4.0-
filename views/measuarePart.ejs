            var urlValue= window.location.search;
            var urlParams = new URLSearchParams(urlValue);
            var id = urlParams.get('partId');
            var inj_date = document.getElementById('injDate').value;
           
            
            $.ajax({
                url: ip_pc+"/measuare/measuarePart/actionPart",
                method: "POST",
