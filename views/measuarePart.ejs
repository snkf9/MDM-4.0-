                dataType: "JSON",
                success: function(data){
                    var batch_input = `<option selected value="0">New Batch</option>
                    `
                    var max_batch;
                    if(data.data.recordset.length >0){
                    for(var l = 1; l <= data.data.recordset.length ; l++){
                        batch_input += `<option value="`+data.data.recordset[l]+`">`+data.data.recordset[l]+`</option>
                        `
