<!doctype html>
<html lang="en">
    <head>

        <!-- Required meta tags -->
        <meta charset="utf-8" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1, shrink-to-fit=no"
        />

        <!-- Bootstrap CSS v5.2.1 -->
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
            crossorigin="anonymous"
        />
        <title>MDM SANTOMAS VIỆT NAM</title>

    </head>

    <body>
        <header>
            <!-- place navbar here -->
            <nav class="navbar navbar-expand-sm navbar-light" style="background-color: #e3f2fd;">
                <label style="font-weight:bolder ; color: red;"><p style="margin-block-start: 0em; margin-block-end: 0em; margin-right: 1em">CÔNG TY SANTOMAS VIỆT NAM JSC</p><p style="margin-block-start: 0em; margin-block-end: 0em">Phần mềm đo tự động</p></label>
                <button class="navbar-toggler d-lg-none" type="button" data-toggle="collapse" data-target="#collapsibleNavId" aria-controls="collapsibleNavId"
                    aria-expanded="false" aria-label="Toggle navigation"></button>
                <div class="collapse navbar-collapse" id="collapsibleNavId">
                    <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                        
                        <li class="nav-item active">
                            <form method="GET" action="/tool" class="form-inline my-2 my-lg-0">
                                <button class="btn btn-secondary btn-lg" type="submit" style="margin-right: 0.5em;">TOOLS</button>
                            </form>
                        </li>

                        <li class="nav-item active">
                            <form method="GET" action="/manager" class="form-inline my-2 my-lg-0">
                                <button class="btn btn-secondary btn-lg" type="submit" style="margin-right: 0.5em;">USERS</button>
                            </form>
                        </li>

                        <li class="nav-item active">
                            <form method="GET" action="/partData" class="form-inline my-2 my-lg-0">
                                <button class="btn btn-secondary btn-lg" type="submit" style="margin-right: 0.5em;">PART DATA</button>
                            </form>
                        </li>

                        <li class="nav-item active">
                            <form method="GET" action="/measuare" class="form-inline my-2 my-lg-0">
                                <button class="btn btn-secondary btn-lg" type="submit" style="margin-right: 0.5em;">MEASUARE</button>
                            </form>
                        </li>

                        <li class="nav-item active">
                            <form method="GET" action="/checkSheet" class="form-inline my-2 my-lg-0">
                                <button class="btn btn-secondary btn-lg" type="submit" style="margin-right: 0.5em;">CHECK SHEET</button>
                            </form>
                        </li>

                        

                    </ul>
                    
                </div>
            </nav>
        </header>
        <main>
            <div class="container">
                <h1 class="mt-4 mb-4 text-center text-primary"><b></b></h1>
                <span id="mess_data"></span>
                <div class="card">
                    <div class="card-header">
                        <div class="row">
                            <div class="col col-sm-9">PART DATA TABLE</div>
                            <div class="col col-sm-3">
                                <button type="button" id="add_part" class="btn-success btn-sm float-end">Add</button>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered" id="part_data_table">
                                <thea>
                                    <tr>
                                        <th>Id</th>
                                        <th>Mold No</th>
                                        <th>Part Name</th>
                                        <th>Part No</th>
                                        <th>Die No</th>
                                        <th>Factory</th>
                                        <th>Date Added</th>
                                    </tr>
                                </thea>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <footer>
            <!-- place footer here -->
        </footer>
        <!-- Bootstrap JavaScript Libraries -->
        <script
            src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
            integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
            crossorigin="anonymous"
        ></script>

        <script
            src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"
            integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+"
            crossorigin="anonymous"
        ></script>
    </body>
</html>

<div class="modal" tabindex="-1" id="partdata_action_modal">
    <div class="modal-dialog">
        <div class="modal-content">
            <form method="post" url="http://localhost:3000/partdata/addPartdata" body="partDataJson" id="partdata_form">
                <div class="modal-header" id="partdata_dynamic_modal_title">
                    <h5 class="modal-title"></h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>    
                </div>
                <div class="modal-body" >
                    <div class="mb-3">
                        <label class="form-label">Mold No</label>
                        <input type="text" name="mold_no"  id="mold_no" class="form-control" />
                    </div>

                    <div class="mb-3">
                        <label class="form-label">Part Name</label>
                        <input type="text" name="part_name"  id="part_name" class="form-control" />
                    </div>
                
                    <div class="mb-3">
                        <label class="form-label">Part No</label>
                        <input type="text" name="part_no"  id="part_no" class="form-control" />
                    </div>

                    <div class="mb-3">
                        <label class="form-label">Dia No</label>
                        <input type="text" name="dia_no"  id="dia_no" class="form-control" />
                    </div>

                    <div class="mb-3" >
                        <label class="form-label">Factory No</label>
                        <select  id="factory_p" name="factory_p" class="form-control">
                            <option value="SVN2">SVN2</option>
                            <option value="SVN3">SVN3</option>
                        </select>
                    </div>

                </div>

                <div class="modal-footer">
                    <input type="hidden" name="id" id="id" />
                    <input type="hidden" name="actionPart" id="actionPart" value="Add" />
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary" id="action_buttonpartdata" >Add</button>
                </div>
            </form>
        </div>
    </div>
</div>
